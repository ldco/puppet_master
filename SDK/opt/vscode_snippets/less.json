{

    "scrollbars": {
        "prefix": "log",
        "body": [
            "scrolbars",
            "//bar color",
            "::-webkit-scrollbar-thumb {",
            "background-color: @_Color1;",
            "border-radius: 4px;",
            "}",
            "//bar color hover",
            "::-webkit-scrollbar-thumb:hover {",
            "background-color: @_Color4;",
            "}",
            "//width",
            "::-webkit-scrollbar {",
            "width: 2vw;",
            "}",
            "//button is OFF",
            "::-webkit-scrollbar-button {",
            "display: none;",
            "height: 13px;",
            " border-radius: 0px;",
            "background-color: @_bg_Dark;",
            "}",
            "//button hover",
            "::-webkit-scrollbar-button:hover {",
            "background-color: @_bg_Dark;",
            "}",
            "//background",
            "::-webkit-scrollbar-track {",
            "background-color: transparent;",
            "}",
            "//background hover",
            "::-webkit-scrollbar-track:hover {",
            "background-color: transparent;",
            "}"
        ],
        "description": "custom scrollbars"
    },
    "flex": {
        "prefix": "flexCenter",
        "body": [
            "display: flex;",
            "justify-content: $1center;",
            "align-items: $2center;"
        ],
        "description": "custom scrollbars"
    },
    "boxshadow": {
        "prefix": "boxshadow",
        "body": [
            "box-shadow: $11px $21px $31px $40px $5#444;"
        ],
        "description": "boxshadow",
    },
    "gridTemplColumns": {
        "prefix": "grid-template-columns",
        "body": [
            "grid-template-columns: 1.2fr 1.2fr 1.2fr 1.2fr 2fr 0.2fr 0.2fr;"
        ],
        "description": "grid-template-columns"
    },
    "backgroundColor": {
        "prefix": "bgColor",
        "body": [
            "background: @_Color2$1;"
        ],
        "description": "background color"
    },
    "backgroundImage": {
        "prefix": "bgImage",
        "body": [
            "background: @_Color2$1;",
            "background-image: url(../icons/menuIconsVectors/$2.svg);",
            "background-repeat: no-repeat;",
            "background-position-y: center;",
            "background-position-x: center;",
            "background-size: $3contain;"

        ],
        "description": "background image"
    }



}