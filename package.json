{
    "name": "PuppetMaster",
    "version": "0.1.0",
    "description": "webframework",
    "main": "index.php",
    "author": "Louis David & Co.",
    "license": "LD&CO Commercial License 1.0",
    "homepage": "puppetmaster.pro",
    "sysFolder": {
        "folder": "sys"
    },
    "sysFolder2": {
        "folder": "core"
    },
    "scripts": {
        "copy--img-icons": "./etc/sh/copy--img_icons.sh",
        "pngquant": "./etc/sh/pngquant.sh",
        "svgo--ico": "svgo -f ./$npm_package_sysFolder_folder/assets/icons/vector -r -o ./$npm_package_sysFolder_folder/assets/icons/vector --multipass",
        "svgo--imgs": "svgo -f ./$npm_package_sysFolder_folder/assets/images/images -r -o ./$npm_package_sysFolder_folder/assets/images/images --multipass",
        "sass--dev-l": "node-sass -w $npm_package_sysFolder_folder/styles/sass/master-l.scss www/master-l.css",
        "sass--dev-r": "node-sass -w $npm_package_sysFolder_folder/styles/sass/master-d.scss www/master-d.css",
        "sass--prod-l": "node-sass $npm_package_sysFolder_folder/styles/sass/master-l-prod.scss www/master-l-prod.css",
        "sass--prod-r": "node-sass $npm_package_sysFolder_folder/styles/sass/master-d-prod.scss www/master-d-prod.css",
        "autoprefixer-l": "postcss www/master-l-prod.css --use=autoprefixer --map=false --output=www/master-l-prod.css",
        "autoprefixer-r": "postcss www/master-d-prod.css --use=autoprefixer --map=false --output=www/master-d-prod.css",
        "cleancss-l": "cleancss --level 2 --output www/master-l-prod.min.css www/master-l-prod.css",
        "cleancss-r": "cleancss --level 2 --output www/master-d-prod.min.css www/master-d-prod.css",
        "babel--dev": "babel $npm_package_sysFolder_folder/JS -o www/babels.js -w",
        "babel--prod": "babel $npm_package_sysFolder_folder/JS -o www/babels.js",
        "browserify": "browserify -p common-shakeify www/babels.js -o www/master.js",
        "watchify": "watchify www/babels.js -o www/master.js",
        "uglify": "uglifyjs www/master.js -o www/master.min.js -c",
        "dev": "npm run php && concurrently 'npm:babel--dev' 'npm:sass--dev-l' 'npm:sass--dev-r' 'npm:watchify'",
        "deploy--vendor": "./vendor/dg/ftp-deployment/deployment ../ftpdeploy/deploy--vendor.php",
        "deploy--dist": "./vendor/dg/ftp-deployment/deployment ../ftpdeploy/deploy--dist.php",
        "deploy--js": "./vendor/dg/ftp-deployment/deployment ../ftpdeploy/deploy--js.php",
        "deploy--css": "./vendor/dg/ftp-deployment/deployment ../ftpdeploy/deploy--css.php",
        "deploy--assets": "./vendor/dg/ftp-deployment/deployment ../ftpdeploy/deploy--assets.php",
        "deploy--icons": "./vendor/dg/ftp-deployment/deployment ../ftpdeploy/deploy--icons.php",
        "deploy--all": "npm run deploy--vendor && npm run deploy--dist",
        "git": "./etc/sh/git--main.sh",
        "g": "./etc/sh/git--master.sh",
        "git--self": "./etc/sh/git--self--deploy.sh",
        "dist--assets": "npm run copy--img-icons && npm run pngquant && npm run svgo--ico && npm run svgo--imgs",
        "dist--js": "npm run babel--prod && npm run browserify && npm run uglify",
        "dist--css": "npm run sass--prod-l && npm run sass--prod-r && npm run autoprefixer-l &&  npm run autoprefixer-r && npm run cleancss-l && npm run cleancss-r",
        "dist-build": "./etc/sh/dist-build.sh",
        "dist": "npm run dist--assets && npm run dist--css && npm run dist--js && npm run dist-build",
        "dep--init": "npm run dist && npm run git--self && npm run deploy--all",
        "dep--dist": "npm run dist && npm run git--self && npm run deploy--dist",
        "dep--js": "npm run dist--js && npm run git--self && npm run deploy--js",
        "dep--css": "npm run dist--css && npm run git--self && npm run deploy--css",
        "dep-assets": "npm run dist-assets && npm run git--self && npm run deploy-assets",
        "php": "./etc/sh/phpServer.sh",
        "phpkill": "killall -9 php"
    },
    "browserslist": [
        "last 4 versions"
    ],
    "repository": {
        "type": "git",
        "url": ""
    },
    "dependencies": {
        "aos": "^2.3.4",
        "babelify": "^10.0.0",
        "lodash": "^4.17.15",
        "macy": "^2.5.1",
        "plyr": "^3.5.10",
        "quill": "^1.3.7",
        "redom": "^3.25.1",
        "swiper": "^5.3.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.6",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-transform-arrow-functions": "^7.2.0",
        "@babel/preset-env": "^7.8.6",
        "autoprefixer": "^9.7.4",
        "browserify": "^16.5.0",
        "clean-css-cli": "^4.3.0",
        "common-shakeify": "^0.6.2",
        "concat": "^1.0.3",
        "concurrently": "^5.1.0",
        "less": "^3.11.1",
        "less-watch-compiler": "^1.14.1",
        "node-sass": "^4.13.1",
        "postcss-cli": "^7.1.0",
        "svgo": "^1.3.2",
        "uglify-js": "^3.7.2",
        "watchify": "^3.11.1"
    },
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        "@babel/preset-env",
                        {}
                    ]
                }
            ]
        ]
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "esmodules": true
                    }
                }
            ]
        ],
        "plugins": [
            [
                "@babel/plugin-transform-arrow-functions"
            ],
            [
                "@babel/plugin-proposal-class-properties"
            ]
        ]
    }
}