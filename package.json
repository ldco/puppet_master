{
    "name": "PuppetMaster",
    "version": "0.1.0",
    "description": "webframework",
    "main": "index.php",
    "author": "Louis David & Co.",
    "license": "LD&CO Commercial License 1.0",
    "homepage": "puppetmaster.pro",
    "sysFolder": {
        "folder": "sys"
    },
    "sysFolder2": {
        "folder": "core"
    },
    "scripts": {
        "copy--img-icons": "./SDK/sh/copy--img_icons.sh",
        "pngquant": "./SDK/sh/pngquant.sh",
        "cwebp": "./SDK/sh/cwebp.sh",
        "svgo--ico": "svgo -f ./$npm_package_sysFolder_folder/assets/icons/vector -r -o ./$npm_package_sysFolder_folder/assets/icons/vector --multipass",
        "svgo--imgs": "svgo -f ./$npm_package_sysFolder_folder/assets/images/images -r -o ./$npm_package_sysFolder_folder/assets/images/images --multipass",
        "sass--dev-l": "node-sass -w $npm_package_sysFolder_folder/styles/sass/master-l.scss www/master-l.css",
        "sass--dev-r": "node-sass -w $npm_package_sysFolder_folder/styles/sass/master-d.scss www/master-d.css",
        "sass--prod-l": "node-sass $npm_package_sysFolder_folder/styles/sass/master-l-prod.scss www/master-l-prod.css",
        "sass--prod-r": "node-sass $npm_package_sysFolder_folder/styles/sass/master-d-prod.scss www/master-d-prod.css",
        "autoprefixer-l": "postcss www/master-l-prod.css --use=autoprefixer --map=false --output=www/master-l-prod.css",
        "autoprefixer-r": "postcss www/master-d-prod.css --use=autoprefixer --map=false --output=www/master-d-prod.css",
        "cleancss-l": "cleancss --level 2 --output www/master-l-prod.min.css www/master-l-prod.css",
        "cleancss-r": "cleancss --level 2 --output www/master-d-prod.min.css www/master-d-prod.css",
        "babel--dev": "babel $npm_package_sysFolder_folder/JS -o www/babels.js -w",
        "babel--prod": "babel $npm_package_sysFolder_folder/JS -o www/babels.js",
        "browserify": "browserify -p common-shakeify www/babels.js -o www/master.js",
        "watchify": "watchify www/babels.js -o www/master.js",
        "uglify": "uglifyjs www/master.js -o www/master.min.js -c",
        "dev": "npm run start--dev && concurrently 'npm:babel--dev' 'npm:sass--dev-l' 'npm:sass--dev-r' 'npm:watchify'",
        "deploy--vendor": "./vendor/dg/ftp-deployment/deployment deploy/deploy--vendor.php",
        "deploy--dist": "./vendor/dg/ftp-deployment/deployment deploy/deploy--dist.php",
        "deploy--assets": "./vendor/dg/ftp-deployment/deployment deploy/deploy--assets.php",
        "deploy--noassets": "./vendor/dg/ftp-deployment/deployment deploy/deploy--noassets.php",
        "deploy--icons": "./vendor/dg/ftp-deployment/deployment deploy/deploy--icons.php",
        "deploy--www": "./vendor/dg/ftp-deployment/deployment deploy/deploy--www.php",
        "deploy--all": "npm run deploy--vendor && npm run deploy--dist",
        "git": "./SDK/sh/git--main.sh",
        "g": "./SDK/sh/git--master.sh",
        "git--self": "./SDK/sh/git--self--deploy.sh",
        "predist--assets": "npm run copy--img-icons && npm run pngquant && npm run cwebp && npm run svgo--ico && npm run svgo--imgs",
        "predist--js": "npm run babel--prod && npm run browserify && npm run uglify",
        "predist--css": "npm run sass--prod-l && npm run sass--prod-r && npm run autoprefixer-l &&  npm run autoprefixer-r && npm run cleancss-l && npm run cleancss-r",
        "predist--noassets": "npm run predist--js && npm run predist--css",
        "dist": "./SDK/sh/dist.sh",
        "build": "npm run predist--assets && npm run predist--noassets && npm run dist",
        "build--noassets": "npm run predist--noassets && npm run dist",
        "dep--init": "npm run build && npm run git--self && npm run deploy--all",
        "dep--dist": "npm run build && npm run git--self && npm run deploy--dist",
        "dep--assets": "npm run predist-assets && npm run dist && npm run git--self && npm run deploy--assets",
        "dep--noassets": "npm run predist--noassets && npm run build--noassets && npm run git--self && npm run deploy--noassets",
        "dep--www": "npm run predist--noassets && npm run build--noassets && npm run git--self && npm run deploy--www",
        "start--dev": "./SDK/sh/start--dev.sh"
    },
    "browserslist": [
        "last 4 versions"
    ],
    "repository": {
        "type": "git",
        "url": ""
    },
    "dependencies": {
        "aos": "^2.3.4",
        "babelify": "^10.0.0",
        "lodash": "^4.17.15",
        "macy": "^2.5.1",
        "plyr": "^3.6.2",
        "quill": "^1.3.7",
        "redom": "^3.27.0",
        "swiper": "^5.4.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.10.2",
        "@babel/plugin-proposal-class-properties": "^7.10.1",
        "@babel/plugin-transform-arrow-functions": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "autoprefixer": "^9.8.0",
        "browserify": "^16.5.1",
        "clean-css-cli": "^4.3.0",
        "common-shakeify": "^0.6.2",
        "concat": "^1.0.3",
        "concurrently": "^5.2.0",
        "node-sass": "^4.14.1",
        "postcss-cli": "^7.1.1",
        "svgo": "^1.3.2",
        "uglify-js": "^3.9.4",
        "watchify": "^3.11.1"
    },
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        "@babel/preset-env",
                        {}
                    ]
                }
            ]
        ]
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "esmodules": true
                    }
                }
            ]
        ],
        "plugins": [
            [
                "@babel/plugin-transform-arrow-functions"
            ],
            [
                "@babel/plugin-proposal-class-properties"
            ]
        ]
    }
}