{

    "rtl/ltr": {
        "prefix": "dir",
        "body": [
            "[dir=rtl] & {$1}",
            "[dir=ltr] & {$2}"
        ],
        "description": "direction rtl/ltr"
    },
    "mob query": {
        "prefix": "mob",
        "body": [
            "@include mob(true) {",
            "$1#someid {$2display: none !important;}}",
            "@include mob(false) {",
            "$3#someid{$4display: none !important;}}"
        ],
        "description": "mob query"
    },
    "box shadow": {
        "prefix": "bs",
        "body": [
            "box-shadow:$1 0$2 0$3 2$4 2 $5\\$c_shadow;"
        ],
        "description": "box shadow css"
    },
    "display flex": {
        "prefix": "fc",
        "body": [
            "display: flex;",
            "justify-content: $1center;",
            "align-items: $2center;",
        ],
        "description": "display flex all center"
    },
    "calc": {
        "prefix": "calc",
        "body": [
            "calc(#{$1} / #{$2})"
        ],
        "description": "calc"
    },
    "widthheight": {
        "prefix": "wh",
        "body": [
            "width: $1vw;",
            "height: $1vh;",
        ],
        "description": "width & height"
    },
    "fontSize": {
        "prefix": "fs",
        "body": [
            "@include f-size($1\\$basicTextSize);"

        ],
        "description": "pm font size"
    },
    "lightTheme": {
        "prefix": "theme",
        "body": [
            "@if \\$lightThemeBool == true {",
            " ",
            "}",
            "@if \\$lightThemeBool == false {",
            " ",
            "}",

        ],
        "description": "Light Theme"
    }

}